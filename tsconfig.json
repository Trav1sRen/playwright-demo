{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./test-out"
  },
  "exclude": ["./node_modules"]
}
